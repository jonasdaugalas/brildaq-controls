        {
            "name": "dipPubs",
            "hideInOverview": true,
            "type": "commaSeparatedString",
            "xpath": ".//xc:Application[@class='bril::dip::CommonDataExchange']/cde:properties/cde:dipPubs"
        },
        {
            "name": "flashlists",
            "type": "commaSeparatedString",
            "xpath": ".//xc:Application[@class='bril::dip::CommonDataExchange']/cde:properties/cde:flashlists",
